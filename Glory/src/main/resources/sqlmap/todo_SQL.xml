<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="todo">

<insert id="insert"
	parameterType="hashMap">
	<![CDATA[
		INSERT INTO glorytodo (todo, tdue) VALUES (#{todo}, #{tdue})
	]]>
</insert>

<select id="select_list" resultType="com.glory.main.Todo">
<![CDATA[
	SELECT * FROM glorytodo ORDER BY tid DESC
	]]>
</select>

</mapper>